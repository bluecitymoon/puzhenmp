            <icon type="success" size="93"></icon>
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">操作成功</view>
            <view class="weui-msg__desc">您的手机号<b>{{phoneNumber}}</b>已成功预约免费体验课！</view>
            <view class="weui-msg__title" wx:if="{{giftCode}}">礼物码: {{giftCode}}</view>
            <view class="weui-msg__desc" wx:if="{{giftCode}}">为方便礼物领取，请截屏保存礼物兑换码</view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <!-- <navigator url='pages/introduction/index' class="weui-btn" type="primary">关闭</navigator> -->
                <!-- <button class="weui-btn" type="default">辅助操作</button> -->
            </view>
        </view>
        <view class="weui-msg__extra-area">
            <view class="weui-footer">
                <view class="weui-footer__links">
                    <!-- <navigator url="" class="weui-footer__link">底部链接文本</navigator> -->
                </view>
                <view class="weui-footer__text">Copyright © 2008-2018 朴真国学</view>
            </view>
        </view>
    </view>
</view>